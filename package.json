{
  "name": "the-anwer",
  "version": "1.0.0",
  "description": "the-anwer",
  "main": "./dist/electron/main/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./.electron-vite/build.js",
    "publish": "node ./.electron-vite/build.js && electron-builder",
    "build:win64": "node ./.electron-vite/build.js && electron-builder --win --x64",
    "build:win32": "node ./.electron-vite/build.js && electron-builder --win --ia32",
    "dev": "node ./.electron-vite/dev-runtime.js"
  },
  "build": {
    "directories": {
      "output": "build"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1"
      }
    ],
    "files": [
      "dist/electron/**/*",
      "!node_modules"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.16.3",
    "electron-updater": "^4.3.9",
    "js-yaml": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "bytenode": "^1.3.3",
    "cfonts": "^2.9.3",
    "chalk": "^1.1.3",
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7",
    "less": "^4.1.1",
    "multispinner": "^0.2.1",
    "portfinder": "^1.0.28",
    "rollup": "^2.52.1",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "vite": "^2.3.7"
  }
}
